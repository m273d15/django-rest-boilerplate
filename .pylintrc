[MISCELLANEOUS]

# List of note tags to take in consideration, separated by a comma.
notes=FIXME,XXX


[BASIC]
const-name-hint=(([A-Z_][A-Z0-9_]*)|(__.*__)|([a-z_][a-z0-9_]*))$
# Naming hint for function names
function-name-hint=(([a-z][a-z0-9_]{2,35})|(_[a-z0-9_]*))$

# Regular expression matching correct function names
function-rgx=(([a-z][a-z0-9_]{2,35})|(_[a-z0-9_]*))$

# Naming hint for method names
method-name-hint=(([a-z][a-z0-9_]{2,35})|(_[a-z0-9_]*))$

# Regular expression matching correct method names
method-rgx=(([a-z][a-z0-9_]{2,35})|(_[a-z0-9_]*))$


[FORMAT]

# Maximum number of characters on a single line.
max-line-length=120
